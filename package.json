    "url": "https://github.com/mouten57/texas-math-central/issues"
  },
  "homepage": "https://github.com/mouten57/texas-math-central#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cloudinary": "^1.12.0",
    "concurrently": "^4.0.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "googleapis": "^27.0.0",
    "lodash": "^4.17.11",
